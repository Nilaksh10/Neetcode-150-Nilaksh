class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        l=0
        charSet = set()
        res = 0
        for r in range(len(s)):
            while s[r] in charSet:
                charSet.remove(s[l])
                l+=1
            charSet.add(s[r])
            res = max(res,r-l+1)  #r-l+1 to determine size of window
        return res
